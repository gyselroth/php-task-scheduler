{
    "name": "gyselroth/mongodb-php-task-scheduler",
    "description": " Asynchronous task scheduler for PHP based on MongoDB",
    "keywords": ["mongodb", "async", "task-scheduler"],
    "homepage": "http://www.github.com/gyselroth/mongodb-php-task-scheduler",
    "license": "MIT",
    "authors": [
        {
            "name": "Raffael Sahli",
            "email": "sahli@gyselroth.net"
        }
    ],
    "autoload": {
        "psr-4": {
            "TaskScheduler\\": "src",
            "TaskScheduler\\Testsuite\\": "tests"
        }
    },
    "require": {
        "php": ">=7.1",
        "psr/log": "1.*",
        "psr/container": "*",
        "ext-mongodb": "*",
        "mongodb/mongodb": "*"
    },
    "repositories": [
         {
             "type": "git",
             "url": "https://github.com/raffis/php-mongomock"
         }
    ],
    "require-dev": {
        "phpunit/phpunit": "5.7.*",
        "friendsofphp/php-cs-fixer": "*",
        "helmich/mongomock": "dev-master#48e11f1",
        "phpstan/phpstan": "^0.8.5"
    }
}
